{
  "hosting": {
    "public": "public/stylesheets",
    "rewrites": [{
      "source": "**",
      "function": "app"
    }],
    "functions": {
      "source": "./index.js",
      "runtime": "nodejs16"
    },
    "ignore": [
      "firebase.json",
      "**/node_modules/**",
      "**/.env/**"
    ]
  }
}