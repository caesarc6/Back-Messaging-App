{
	"hosting": {
		"public": "public/stylesheets", 
    "rewrites": [
      {
          "source": "**",
          "function": "app"
      }
  ],
    
    "redirects": [ {
      "source": "/foo",
      "destination": "public/stylesheets/views/users",
      "type": 301
    } ],

    "functions": {
      "source" : "./",
      "runtime" : "nodejs16"
    },
  
		"ignore": [
			"firebase.json",
			"**/node_modules/**",
      "**/.env/**"

		]
	}
}